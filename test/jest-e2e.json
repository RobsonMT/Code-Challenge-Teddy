{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../",
  "testRegex": "(/test/.*\\.e2e-spec)\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "tsconfig": "tsconfig.json"
    }
  },
  "testTimeout": 30000
}